{
  "name": "gtfs-to-html",
  "version": "1.4.13",
  "private": false,
  "description": "Build human readable transit timetables as HTML or PDF from GTFS",
  "keywords": [
    "transit",
    "gtfs",
    "transportation",
    "timetables"
  ],
  "homepage": "https://gtfstohtml.com",
  "bugs": {
    "url": "https://github.com/blinktaginc/gtfs-to-html/issues"
  },
  "repository": "git://github.com/blinktaginc/gtfs-to-html",
  "license": "MIT",
  "author": "Brendan Nee <brendan@blinktag.com>",
  "contributors": [
    "Evan Siroky <evan.siroky@yahoo.com>",
    "Nathan Selikoff",
    "Aaron Antrim <aaron@trilliumtransit.com>",
    "Thomas Craig <thomas@trilliumtransit.com>",
    "Holly Kvalheim",
    "Pawajoro"
  ],
  "main": "index",
  "bin": {
    "gtfs-to-html": "bin/gtfs-to-html.js"
  },
  "scripts": {
    "start": "node ./app",
    "test": "xo"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "space": true
  },
  "dependencies": {
    "@turf/helpers": "^6.3.0",
    "@turf/simplify": "^6.3.0",
    "archiver": "^5.3.0",
    "chalk": "^4.1.0",
    "cli-table": "^0.3.6",
    "express": "^4.17.1",
    "fs-extra": "^9.1.0",
    "gtfs": "^2.4.2",
    "js-beautify": "^1.13.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "pretty-error": "^3.0.3",
    "progress": "^2.0.3",
    "pug": "^3.0.2",
    "puppeteer": "^8.0.0",
    "sanitize-filename": "^1.6.3",
    "sqlstring": "^2.3.2",
    "timer-machine": "^1.1.0",
    "toposort": "^2.0.2",
    "untildify": "^4.0.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "xo": "^0.38.2"
  },
  "engines": {
    "node": ">= 12.14.0"
  }
}
